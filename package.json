{
  "name": "dissimulare",
  "version": "0.3.1",
  "description": "Simple mask library",
  "source": "src/index.js",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js",
  "sideEffects": false,
  "license": "MIT",
  "keywords": [
    "mask",
    "input mask",
    "text mask",
    "input formatting",
    "string formatting",
    "text formatting"
  ],
  "author": {
    "email": "nospam@gmail.com",
    "name": "FÃ¡bio de Carvalho",
    "url": "https://github.com/callmefabioo"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/callmefabioo/dissimulare"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:clean && microbundle && npm run size",
    "build:clean": "rimraf dist",
    "size": "size-limit",
    "dev": "microbundle watch",
    "test": "jest --verbose false --silent --maxWorkers=2 --no-cache",
    "test:dev": "jest --watch",
    "pretest": "npm run build",
    "prerelease": "npm run build && npm run test",
    "version:patch": "release patch",
    "version:minor": "release minor",
    "version:major": "release major",
    "version:canary": "release pre canary"
  },
  "dependencies": {
    "memoize-one": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@size-limit/preset-small-lib": "^4.4.5",
    "@types/jest": "^25.2.1",
    "babel-jest": "^25.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.5",
    "microbundle": "^0.12.3",
    "prettier": "^2.0.4",
    "release": "^6.1.0",
    "rimraf": "^3.0.2",
    "size-limit": "^4.4.5"
  },
  "size-limit": [
    {
      "limit": "650 B",
      "path": "dist/**/index.js"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "prettier --write",
      "jest --findRelatedTests --coverage"
    ]
  }
}
